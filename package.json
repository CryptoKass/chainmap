{
  "name": "@vandeurenglenn/chainmap",
  "description": "A comprehensive map of EVM blockchain networks and their associated data. It includes information such as chain IDs, network names, RPC URLs, and currency details.",
  "version": "0.1.2",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./chainmap.json": "./chainmap.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CryptoKass/chainmap.git"
  },
  "scripts": {
    "update": "npm run generate && npm run build && npm run bump",
    "generate": "bun run scripts/generate.ts",
    "build": "tsc",
    "bump": "bun run scripts/bump.ts",
    "fetch": "bun run scripts/fetch.ts",
    "test": "bun test"
  },
  "files": [
    "dist",
    "README.md",
    "chainmap.json"
  ],
  "devDependencies": {
    "@types/semver": "^7.5.3",
    "bun-types": "latest",
    "ethers": "^6.7.1",
    "jsonschema": "^1.4.1",
    "semver": "^7.5.4",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}